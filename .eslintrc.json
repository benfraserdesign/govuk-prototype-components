{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["plugin:jsdoc/recommended", "standard", "prettier"],
  "ignorePatterns": ["_site", "x-govuk/all.js"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": ["jsdoc", "prettier"],
  "rules": {
    "prettier/prettier": ["error"]
  }
}
