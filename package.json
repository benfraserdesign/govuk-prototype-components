{
  "name": "govuk-prototype-components",
  "description": "GOV.UK Prototype Components contains the code you need to use common or experimental components that are not yet in the GOV.UK Design System.",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/paulrobertlloyd/govuk-prototype-rig.git",
    "directory": "packages/govuk-prototype-components"
  },
  "bugs": {
    "url": "https://github.com/paulrobertlloyd/govuk-prototype-rig/issues"
  },
  "homepage": "https://govuk-prototype-rig.herokuapp.com/docs/components",
  "keywords": [
    "govuk",
    "design system",
    "prototyping",
    "components"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "x-govuk/all.js",
  "sass": "x-govuk/all.scss",
  "exports": {
    ".": {
      "import": "./x-govuk/all.js",
      "require": "./x-govuk/all.js"
    },
    "./decorate": {
      "import": "./lib/decorate.js",
      "require": "./lib/decorate.js"
    }
  },
  "files": [
    "lib",
    "x-govuk"
  ],
  "dependencies": {
    "accessible-autocomplete": "^2.0.4",
    "eventslibjs": "^1.2.0",
    "govuk-frontend": "^4.0.1",
    "lodash": "^4.17.21"
  },
  "publishConfig": {
    "access": "public"
  }
}
